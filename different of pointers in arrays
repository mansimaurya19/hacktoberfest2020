#include<stdio.h>
int main()
{
int a[]={6,2,1,4,5};
int *q;
q=a; //a denotes the 1st element of the array
printf("%p \n",a);
printf("%p \n",q);
q++;
printf("%d \n",a[2]);
printf("%d \n",*(a+2));
printf("%d \n",*(q+1));
printf("%p \n",&a);
printf("%p \n",&q);
printf("%p \n",a+1);
printf("%p \n",&a+1);
return 0;
}
